{
  "version": "0.0.1",
  "scripts": {
    "postinstall": "typings install",
    "jshint": "jshint -c jshint.json config/*.js",
    "tslint": "tslint -c tslint.json  src/**/*.ts",
    "jscs": "jscs -c jscs.json .",
    "lint": "npm run tslint ; npm run jshint ; npm run jscs",
    "clean": "npm cache clean && rm -fr node_modules/ typings/ dist/ reports/",
    "clean:dist": "rm -fr dist/",
    "typings": "typings install",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "watch": "webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.0.2",
    "@angular/compiler": "^2.0.2",
    "@angular/compiler-cli": "^0.6.4",
    "@angular/core": "^2.0.2",
    "@angular/forms": "^2.0.2",
    "@angular/http": "^2.0.2",
    "@angular/platform-browser": "^2.0.2",
    "@angular/platform-browser-dynamic": "^2.0.2",
    "@angular/platform-server": "^2.0.2",
    "@angular/router": "^3.0.2",
    "@angular/upgrade": "^2.0.2",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "jquery": "^3.1.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.17.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "codelyzer": "0.0.19",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "es-module-loader": "^1.2.1",
    "es6-promise-loader": "^1.0.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "jscs": "^3.0.7",
    "jshint": "^2.9.3",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.13.0"
  }
}
